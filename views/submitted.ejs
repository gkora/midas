<% include header.ejs %>
    <div class="container">

      <div class="row">
        <div class="box">
          <div>
            <hr>
            <h2 class="intro-text text-center">New Job <strong>Submission</strong></h2>
            <hr>
						<div class='text-center'>
							<img src='/img/submitted.png' /><br/></br>
							<strong>Job Submitted</strong><br/>
							Job Id  <strong><%= job.id %> </strong><br/></br>
							Email sent to <%= job.email %> with URL to track progress.<br/><br/>
							<h2> Current Status </h2>
							<%if ( error !== null ) { %>

								<b>Error encountered. Please contact MIDAS support.</b>

							<%} else { %>
							<center>
								<table class='text-center pull-center'>
									<tr>
										<td class='text-success'><i class="fa fa-check-square-o"></i></td>
										<td style='padding:0 30px 0 30px;'><i class="fa fa-arrow-right"></i></td>
										<td class='text-muted'><i class="fa fa-square-o"></i></td>
										<td style='padding:0 30px 0 30px;'><i class="fa fa-arrow-right"></i></td>
										<td class='text-muted'><i class="fa fa-square-o"></i></td>
										<td style='padding:0 30px 0 30px;'><i class="fa fa-arrow-right"></i></td>
										<td class='text-muted'><i class="fa fa-square-o"></i></td>
									</tr>
									<tr>
										<td class='text-success'>Job Submitted</td>
										<td>&nbsp;</td>
										<td class='text-muted'>Job In Queue</td>
										<td>&nbsp;</td>
										<td class='text-muted'>Job In Execution</td>
										<td>&nbsp;</td>
										<td class='text-muted'>Job Execution Complete</td>
									</tr>
								</table>
							</center>
							<%} %>



							<!--
							<p class='col-md-4'>
								Your job request has been submitted. An email has been sent with the URL to track the progress of your job. Please
								click the link provided in your email to locate your job. Based on the current queue status your job will be completed 
								in 4-5 hours. Please check back with us soon. Save this job id to retrive your job quickly.
							</p>
							-->

							
						</div>

          </div>
        </div>
      </div>

    </div><!-- /.container -->

<% include footer.ejs %>

<!-- JavaScript -->
<script src="/js/jquery-1.10.2.js"></script>
<script src="/js/bootstrap.js"></script>
</body>
</html>
